{
    "name": "gensketch-js",
    "private": true,
    "description": "GenSketch Frontend",
    "version": "0.0.0",
    "type": "module",
    "license": "MIT",
    "browserlist": [
        "defaults",
        "not IE 11",
        "safari >13",
        "not op_mini all"
    ],
    "scripts": {
        "build": "tsc && vite build",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "dev": "vite",
        "preview": "vite preview",
        "tauri": "RUST_LOG=debug tauri",
        "tauri-dev": "RUST_LOG=debug tauri dev",
        "typecov": "type-coverage --strict --detail --ignore-catch | grep -v error"
    },
    "dependencies": {
        "@tauri-apps/api": "^1.2.0",
        "pino": "^8.7.0",
        "pixi-viewport": "^5.0.1",
        "pixi.js": "^7.2.3",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@sveltejs/adapter-auto": "^2.0.0",
        "@sveltejs/adapter-static": "^1.0.0-next.50",
        "@sveltejs/kit": "^1.5.0",
        "@tauri-apps/cli": "^1.2.0",
        "@trivago/prettier-plugin-sort-imports": "^4.1.1",
        "@types/cookie": "^0.5.1",
        "@types/node": "^18.11.9",
        "@types/uuid": "^9.0.0",
        "@typescript-eslint/eslint-plugin": "^5.42.1",
        "@typescript-eslint/parser": "^5.42.1",
        "autoprefixer": "^10.4.13",
        "eslint": "^8.35.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-compat": "^4.1.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-import-alias": "^1.2.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-svelte": "^2.21.0",
        "jest": "^29.5.0",
        "pino-pretty": "^9.1.1",
        "prettier": "^2.7.1",
        "prettier-plugin-svelte": "^2.9.0",
        "svelte": "^3.54.0",
        "svelte-check": "^3.0.1",
        "svelte-preprocess": "^5.0.1",
        "tslib": "^2.4.1",
        "type-coverage": "^2.24.1",
        "typescript": "^4.9.3",
        "vite": "^4.0.0"
    }
}
